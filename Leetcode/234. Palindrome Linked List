Input: head = [1,2,2,1]
Output: true
-----------------

class Solution {
    public boolean isPalindrome(ListNode head) {
        ListNode temp=head;
        Stack<Integer> s= new Stack<>();
        while(temp!=null){
            s.push(temp.val);
            temp=temp.next;
        }
        while(head!=null){
            int p=s.pop();
            if(head.val!=p) return false;
            head=head.next;
        }
        return true;
    }
}
